#!/usr/bin/env python

import rospy
from donkey_actuator.actuator import Actuator, ActuatorException


if __name__ == '__main__':
    try:
        rospy.init_node('donkey_actuator')
        a = Actuator()
        rospy.spin()
    except ActuatorException:
        pass
    except rospy.ROSInterruptException:
        pass
